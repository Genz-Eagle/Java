import java.util.function.Predicate;
//PredicateInterface only have 1 abstract method which has test()

public class PredicateInterface{
    public static void main(String[] args) {
        Predicate<Integer> obj = (no) ->{
            return no%2==0;
        };
        System.out.println(obj.test(3));
    }
}

